{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "compress:css": "node-sass css/style.concat.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fernandes77/natours.git"
  },
  "author": "Rodrigo",
  "license": "MIT",
  "browserslist": "last 10 versions",
  "bugs": {
    "url": "https://github.com/fernandes77/natours/issues"
  },
  "homepage": "https://github.com/fernandes77/natours#readme",
  "devDependencies": {
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  }
}
